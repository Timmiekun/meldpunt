@using Meldpunt.Models;
@using Meldpunt.Utils;
@{ 
  Layout = "";

  var pages = ViewBag.Pages as List<PageModel>;
  var locations = ViewBag.Locations as IEnumerable<PlaatsModel>;
  
  }
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" 
  xmlns:image="http://www.google.com/schemas/sitemap-image/1.1" 
  xmlns:video="http://www.google.com/schemas/sitemap-video/1.1">
  @foreach (var page in pages)
  {
    <url> 
      <loc>http://meldpuntongedierte.nl@(page.Url)</loc>
    </url>
  }

  @foreach (var location in locations)
  {
    
     <url> 
      <loc>http://meldpuntongedierte.nl/ongediertebestrijding-@location.Gemeentenaam.XmlSafe()</loc> 
      <changefreq>monthly</changefreq>
    </url>
  }
</urlset>